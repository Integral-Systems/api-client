meta {
  name: setup ingest Pipeline maintenance
  type: http
  seq: 3
}

put {
  url: {{Host}}/_ingest/pipeline/logs
  body: json
  auth: basic
}

auth:basic {
  username: {{admin_user}}
  password: {{admin_pass}}
}

body:json {
  {
    "description" : "Log Pipeline",
    "processors" : [
      {
        "set" : {
              "field": "_source.ingest_timestamp",
              "value": "{{{_ingest.timestamp}}}"
        }
      }
    ]
  }
}
